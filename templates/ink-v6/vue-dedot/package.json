{
  "name": "vue-dedot",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build:contract": "pop build ./contract && cp ./contract/target/ink/flipper.json ./src/contracts/",
    "deploy:contract": "pop up ./contract --use-wallet --args false --url wss://testnet-passet-hub.polkadot.io",
    "generate:chain": "dedot chaintypes -w wss://testnet-passet-hub.polkadot.io -o ./src/generated",
    "generate:contract": "dedot typink -m ./src/contracts/flipper.json -o ./src/generated/contract",
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint --fix",
    "postinstall": "npm run generate:chain && npm run generate:contract"
  },
  "dependencies": {
    "@talismn/connect-wallets": "^1.2.8",
    "dedot": "^0.18.5",
    "vue": "^3.5.22"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.0.0",
    "@dedot/chaintypes": "^0.170.0",
    "@iconify-json/mdi": "^1.2.3",
    "@iconify-json/token-branded": "^1.2.29",
    "@iconify/tailwind4": "^1.0.6",
    "@tailwindcss/vite": "^4.1.15",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "daisyui": "^5.3.8",
    "eslint": "^9.38.0",
    "tailwindcss": "^4.1.15",
    "typescript": "~5.9.3",
    "vite": "^7.1.12",
    "vue-tsc": "^3.0.9"
  }
}
